@model IEnumerable<InterfaceServices.DTO.ActivationAbonneDTO>

@{
    ViewBag.Title = "DisplayRequestSucces";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section scripts{

      <script src="~/Scripts/DataTable/other/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/DataTable/Export/JSPrintOK.js"></script>

}


@section Styles{

     <link href="@Url.Content("~/Content/dataTables/demo_table.css")" rel="stylesheet" type="text/css" />
 <link href="@Url.Content("~/Content/dataTables/demo_page.css")" rel="stylesheet" type="text/css" />


    <link href="@Url.Content("~/Scripts/DataTable/Export/buttons.dataTables.min.css")" rel="stylesheet" type="text/css" />

}

<h2>Liste des demandes traitées...</h2>


<div id="divlstDI">
    <p align="left">   
       <a href="@Url.Action("ExportToExcel_DIOK", "Emission")" title="Exporter la liste sous Excel...">
            <img src="~/Images/excel.png" alt="Exporter" width="30" height="30" />
        </a>
        &nbsp;&nbsp;&nbsp;&nbsp;
         <a href="#" id="linkPrint" title="Imprimer la liste..." onclick="printContent_DIOK('divContent')">
            <img src="~/Images/print.jpg" alt="Imprimer" width="40" height="30" />
        </a>
     </p>
 
@*  <a href="#" onclick="window.print(); return false;">Print</a>*@

@*<div id="divContent">*@

  @*@Html.TextBox("cptLstDI",(string)@ViewBag.CptElt)*@
  @Html.Hidden("cptLstDI",(string)@ViewBag.CptElt)
 

<table id="lstDIOK" class="display">
    <thead>
    <tr>
        <th></th>   
         <th>
            @Html.DisplayName("Client")
        </th>
          <th>
            @Html.DisplayName("Identifiant Abonne")
        </th>
         <th>
            @Html.DisplayName("Raccordement")
        </th>
         <th>
            @Html.DisplayName("Compteur")
        </th>         
        <th>
            @Html.DisplayName("Type Demande")
        </th>
             <th>
            @Html.DisplayName("Site")
        </th>
        <th>
            @Html.DisplayName("Exploitation")
        </th>
        
        <th> @Html.DisplayName("DateCreation | DateTraitement")</th>
        <th></th>
    </tr>
        </thead>

    <tbody>
    @foreach (var item in Model)
    {
        <tr>
              <td><img src="~/Content/images/Plus.png" alt="expand/collapse" rel="@item.NumeroCompteur" /> 
                </td>             
            <td>
                @Html.DisplayFor(modelItem => item.Client)
            </td>              <td>
                @Html.DisplayFor(modelItem => item.IdentifiantAbonne)
            </td>
               <td>
                @Html.DisplayFor(modelItem => item.ReferenceRaccordement)
            </td>          
            <td>
                @Html.DisplayFor(modelItem => item.NumeroCompteur)
            </td> 
            <td>
                @Html.DisplayFor(modelItem => item.LibelleTypeDemande)
            </td>  
                <td>
                @Html.DisplayFor(modelItem => item.CodeSite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodeExploitation)
            </td>
            <td>
                  @Html.DisplayFor(modelItem => item.DateCreation) |
                @Html.DisplayFor(modelItem => item.DateExtraction)
            </td>
           
        
        </tr>
    }
        </tbody>
</table>
<br /><br />
 

 </div> 
